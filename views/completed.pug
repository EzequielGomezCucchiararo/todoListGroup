extends layout
block content
  ul.list-group
    - var counter = 0
    each task in tasks
      if task.completionDate !== ""
        - ++counter
        li(id= task.id, class="list-group-item")
          span(style="text-decoration: line-through")= counter + '.\t' +task.body
          p(style="color: #92928f")= task.completionDate

  